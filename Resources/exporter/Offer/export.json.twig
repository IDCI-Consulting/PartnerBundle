{
    "id": "{{ entity.id }}",
    "name": "{{ entity.name }}",
    "offer": "{{ entity.offer }}",
    "phone": "{{ entity.phone }}",
    "mail": "{{ entity.mail }}",
    "location":
    {
        "id": "{{ entity.location.id }}",
        "name": "{{ entity.location.name }}",
        "address": "{{ entity.location.address }}",
        "zip_code": "{{ entity.location.zipCode}}",
        "city": "{{ entity.location.city }}",
        "country": "{{ entity.location.country }}",
    },
    "sociallinks":
    [
    {% for sociallink in entity.sociallinks %}
        {% if loop.first %}
        { "id": "{{ sociallink.id }}", "uri": "{{ sociallink.uri }}" }
        {% else %}
        ,{ "id": "{{ sociallink.id }}", "uri": "{{ sociallink.uri }}" }
        {% endif %}
    {% endfor %}
    ]
}