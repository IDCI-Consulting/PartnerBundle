<event id="{{ entity.id }}">
  <name>{{ entity.name }}</name>
  <mail>{{ entity.mail }}</mail>
  <phone>{{ entity.phone }}</phone>
  <offer>{{ entity.offer }}</offer>
  <location id="{{ entity.location.id}}">
    <name>{{ entity.name }}</name>
    <address>{{ entity.address }}</address>
    <zip_code>{{ entity.zip_code }}</zip_code>
    <city>{{ entity.city }}</city>
    <country>{{ entity.country }}</country>
  </location>
  <social_links>
  {% for socialLink in entitiy.socialLinks %}
    <social_link>{{ socialLink.uri }}</social_link>
  {% endfor %}
  <social_links>
</event>