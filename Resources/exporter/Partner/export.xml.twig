<partner id="{{ entity.id }}">
  <name>{{ entity.name }}</name>
  <mail>{{ entity.mail }}</mail>
  <phone>{{ entity.phone }}</phone>
  <locations>
  {% for location in entity.locations %}
    <location id="{{ location.id}}">
        <name>{{ location.name }}</name>
        <address>{{ location.address }}</address>
        <zip_code>{{ location.zipCode }}</zip_code>
        <city>{{ location.city }}</city>
        <country>{{ location.country }}</country>
    </location>
  {% endfor %}
  </locations>
  <offers>
  {% for offer in entity.offers %}
    <offer id="{{ offer.id }}">{{ offer.name }}</offer>
  {% endfor %}
  </offers>
  <social_links>
  {% for socialLink in entity.socialLinks %}
    <social_link id="{{ socialLink.id }}">{{ socialLink.uri }}</social_link>
  {% endfor %}
  </social_links>
</partner>