{% extends "IDCIPartnerBundle::adminbase.html.twig" %}

{% block body %}

{{ parent() }}

<div class="page-header">
  <h1>{% trans %}Partner creation{% endtrans %}</h1>
</div>

<form action="{{ path('admin_partner_partner_create') }}" method="post" {{ form_enctype(form) }}>

    {% include "IDCIPartnerBundle:Partner:form.html.twig" with{"form": form} %}
    <p>
        <button type="submit" class="btn btn-primary">{% trans %}Create{% endtrans %}</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a class="btn" href="{{ path('admin_partner_partner') }}">
            {% trans %}Back to the list{% endtrans %}
        </a>
    </li>
</ul>
{% endblock body %}
